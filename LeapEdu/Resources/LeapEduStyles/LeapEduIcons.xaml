<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit">
    <toolkit:AppThemeObject 
        Light="frog_background_image_light.svg" 
        Dark="frog_background_image_dark.svg" x:Key="BackgroundFrogImage" />

    <toolkit:AppThemeObject
        Light="open_eye_icon_dark.svg" 
        Dark="open_eye_icon_light.svg" x:Key="OpenEyeIcon" />

    <toolkit:AppThemeObject
        Light="close_eye_icon_dark.svg" 
        Dark="close_eye_icon_light.svg" x:Key="CloseEyeIcon" />

    <toolkit:AppThemeObject 
        Light="question_light_icon.svg" 
        Dark="question_dark_icon.svg" x:Key="QuestionIcon" />

    <toolkit:AppThemeObject 
        Light="error_light_icon.svg" 
        Dark="error_dark_icon.svg" x:Key="ErrorIcon" />
</ResourceDictionary>